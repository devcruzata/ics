<script type="text/javascript">


    function validateForm() {
        var emailRegex = /^[A-Za-z0-9._]*\@@[A-Za-z]*\.[A-Za-z]{2,5}$/;
        if ($("#DBA").val().trim() == "") {

            $("#error_Div_DBA").show();
            return false();
        }
        else if ($("#ContName").val().trim() == "") {
            $("#error_Div_ContName").show();
            $("#error_Div_DBA").hide();
            return false;
        }
        else if ($("#bPhone").val().trim() == "") {
            alert();
            $("#error_Div_bPhone").show();
            $("#error_Div_ContName").hide();
            $("#error_Div_DBA").hide();
            return false;
        }
        else if ($("#bPhone").val().trim().length <10 ) {

            $("#error_Div_bPhone").show();
            $("#error_Div_ContName").hide();
            $("#error_Div_DBA").hide();
            return false;
        }
        else if ($("#Email").val().trim() == "") {
            $("#error_Div_Email").show();
            $("#error_Div_bPhone").hide();
            $("#error_Div_ContName").hide();
            $("#error_Div_DBA").hide();
            return false;
        }
        else if (!emailRegex.test($("#Email").val().trim())) {
            $("#error_Div_Email").show();
            $("#error_Div_bPhone").hide();
            $("#error_Div_ContName").hide();
            $("#error_Div_DBA").hide();
            return false;
        }
        else if ($("#website").val().trim() == "") {
            $("#error_Div_website").show();
            $("#error_Div_Email").hide();
            $("#error_Div_bPhone").hide();
            $("#error_Div_ContName").hide();
            $("#error_Div_DBA").hide();
            return false;
        }       
        else {           
            $("#error_Div_website").hide();
            $("#error_Div_Email").hide();
            $("#error_Div_bPhone").hide();
            $("#error_Div_ContName").hide();
            $("#error_Div_DBA").hide();
            return true;
        }
    }


    function SubmitForm() {

        if (validateForm()) {
            AddLead();
        }
    }

    function Cancel() {
        window.location = '@Url.RouteUrl("ManageLead")';
    }

</script>

<script>
    function AddLead() {
        var form = $('#leadForm');
        $.ajax({
            cache: false,
            async: true,
            type: "POST",
            url: form.attr('action'),
            data: form.serialize(),
            success: function (data) {
                if (data == '0') {
                    $("#leadForm")[0].reset();
                    showSuccessMsg();
                   // closeWindow();
                }

                if (data == '1') {
                    showErrorMsg();
                }

                if (data == '2') {
                    $("#leadForm")[0].reset();
                    showWarningMsg();
                }

                if (data == '3') {
                    $("#leadForm")[0].reset();
                    showWarningMsg();
                }

                if (data == "4") {
                    $("#leadForm")[0].reset();
                    showWarningMsg();
                }
            },
            error: function (xhr) {
                $('#errorMessage').html("No connection");
            }
        });
    }

    function showSuccessMsg() {
        //$("#sweet-3").on("click", function () {
        swal({
            title: "Success!",
            text: "Lead Details Updated Succesfully!",
            type: "success",
            confirmButtonClass: "btn-raised btn-success btn-ok",
            confirmButtonText: "OK"
        })
        
        //})
    }

    function showErrorMsg() {
        //$("#sweet-6").on("click", function () {
        swal({
            title: "Fail!",
            text: "Error Occured in updating Lead!",
            type: "error",
            confirmButtonClass: "btn-raised btn-danger",
            confirmButtonText: "OK"
        })
        //  })
    }

    function showWarningMsg() {       
        swal({
            title: "Warning!",
            text: "Some Lead details is not updated . Pls check!",
            type: "warning",
            confirmButtonClass: "btn-raised btn-warning",
            confirmButtonText: "OK"
        })
       
    }

   
 function closeWindow(){
            window.close();       
    };
   
</script>

<script>
    function isNumberKey(evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode
        if (charCode > 31 && (charCode < 48 || charCode > 57))
            return false;

        return true;
    }

    function zipCheck(evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode
        if (charCode > 31 && (charCode < 48 || charCode > 57)) {
            return false;
        }
        else {
            return true;
        }
    }


</script>

