<script>
    function AddLead() {       
        var form = $('#leadForm');       
        $.ajax({
            cache: false,
            async: true,
            type: "POST",
            url: form.attr('action'),
            data: form.serialize(),
            success: function (data) {
                if (data == '0') {
                    $("#leadForm")[0].reset();
                    showSuccessMsg();
                }

                if (data == '1') {
                    showErrorMsg();
                }

                if (data == '2') {
                    $("#leadForm")[0].reset();
                    showWarningMsg();
                }

                if (data == '3') {
                    $("#leadForm")[0].reset();
                    showWarningMsg();
                }

                if (data == "4") {
                    $("#leadForm")[0].reset();
                    showWarningMsg();
                }               
            },
            error: function (xhr) {
                $('#errorMessage').html("No connection");
            }
        });
    }

