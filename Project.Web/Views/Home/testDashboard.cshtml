    <script>

        function GetUsersForChat() {
            $.ajax({
                url: '/Chat/GetUserForChat',
                type: "POST",
                async: true,
                cache: false,
                success: function (data) {
                    if (data != '') {
                        $("#chatUserList").html('');
                        $("#chatUserList").html(data);
                    }
                    else {
                        location.reload();
                    }
                },
                error: function (xhr) {
                    $('#errorMessage').html("No connection");
                }
            });
        }

        function setReciever(RecieverID, RecieverName) {
            $("#reciever_ID").val(RecieverID);
            //var htmlText = RecieverName + "<a href=\"#\" class=\"conversation-toggle pull-left\"><i class=\"ti-arrow-left text-muted\"></i></a><a href=\"javascript:;\" class=\"pull-right\"><i class=\"ti-pencil text-muted\"></i></a>";
            // $("#chatRecieverName").html(htmlText);
            $(".conversation").toggleClass("closed")

        }

        function renderChat(sender, reciever) {
            $.ajax({
                url: '/Chat/RenderChat',
                type: "POST",
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify({ SenderID: sender, RecieverID: reciever }),
                async: true,
                cache: false,
                success: function (data) {
                    if (data != '') {
                        $("#chatBox").html('');
                        $("#chatBox").html(data);
                        // location.reload();
                    }
                    else {

                        location.reload();
                    }
                },
                error: function (xhr) {
                    $('#errorMessage').html("No connection");
                }
            });
        }

        function sendMsg() {
            $.ajax({
                url: '/Chat/SendMsg',
                dataType: "json",
                type: "POST",
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify({ SenderID: $("#sender_ID").val(), RecieverID: $("#reciever_ID").val(), Msg: $("#Chat_Msg").val() }),
                async: true,
                processData: false,
                cache: false,
                success: function (data) {
                    if (data == 'Success') {
                        $("#Chat_Msg").val('');
                        renderChat($("#sender_ID").val(), $("#reciever_ID").val());
                    }
                    else {
                        renderChat($("#sender_ID").val(), $("#reciever_ID").val());
                    }
                },
                error: function (xhr) {
                    $('#errorMessage').html("No connection");
                }
            });
        }

    </script>
