<script>
    function ValidateStep1() {
        var emailRegex = /^[A-Za-z0-9._]*\@@[A-Za-z]*\.[A-Za-z]{2,5}$/;
        if ($("#MerAppUserName").val().trim() == "") {           
            $("#MerAppUserName").addClass('addBorder');
            return false;
        }
        if (!emailRegex.test($("#MerAppUserName").val().trim())) {
            $("#MerAppUserName").addClass('addBorder');
            return false;
        }
        else if ($("#MerAppPassword").val().trim() == "") {
            $("#MerAppPassword").addClass('addBorder');
            $("#MerAppUsername").removeClass('addBorder');
            return false;
        }        
        else {            
            $("#MerAppPassword").removeClass('addBorder');
            $("#MerAppUsername").removeClass('addBorder');
            return true;;
        }
    }

    $("#b1Next").click(function () {
        
        if (ValidateStep1()) {           
            SubmitStep1Form();
        }
    });

    function SubmitStep1Form() {       
        var formPayment = $('#formStep1');
        $.ajax({
            url: formPayment.attr('action'),
            type: "POST",
            data: formPayment.serialize(),
            async: true,
            processData: false,
            cache: false,
            success: function (data) {
                if (data != '') {
                    $("#AppID").val(data);
                    $("#appIdStep2").val(data);                    
                    $("#b1").removeClass('btn-success');
                    $("#b1").hide();
                    $("#b2").addClass('btn-success');
                    $("#b3").removeClass('btn-success');
                    $("#b4").removeClass('btn-success');
                    $("#b5").removeClass('btn-success');
                    $("#b6").removeClass('btn-success');
                    $("#step1").hide();
                    $("#step2").show();
                    $("#step3").hide();
                    $("#step4").hide();
                    $("#step5").hide();
                    $("#step6").hide();
                }
                else {                  
                }
            },
            error: function (xhr) {
                $('#errorMessage').html("No connection");
            }
        });

    }
</script>

