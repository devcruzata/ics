<script>
    function SubmitApp() {      
            $.ajax({
                url: '/Application/SubmitApplication',
                type: "POST",
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify({ ApplicationId: $("#AppID").val() }),
                async: true,
                processData: false,
                cache: false,
                success: function (data) {
                    if (data != '') {                       
                        Location.reload();
                    }
                    else {                        
                        location.reload();
                    }
                },
                error: function (xhr) {                   
                    $('#errorMessage').html("No connection");
                }
            });       
    }
</script>




































